// For IsiMsgBase
SOURCE          cisimsgbasetest.cpp
SOURCE          cisimsgapitest.cpp

// For CTestLogger
SOURCE          ctestlogger.cpp


CAPABILITY ALL
USERINCLUDE     ../inc

